using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace IntegratedTaxSystem.Utility
{
    public class Node
    {
        public string appId;
        public string text;
        public string id;
        public bool expanded;
        public bool leaf;
        public string iconCls;
        public string link;
        private List<Node> _children = new List<Node>();
        public List<Node> children
        {
            get { return _children; }
            set { _children = value; }
        }
    }
        
}